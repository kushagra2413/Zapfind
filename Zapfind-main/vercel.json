{
  "version": 2,
  "routes": [
    {
      "src": "/firebase-config",
      "dest": "/api/firebase-config.js"
    },
    {
      "src": "/api/slots/([^/]+)",
      "dest": "/api/slots.js?stationId=$1"
    },
    {
      "src": "/api/bookings",
      "dest": "/api/bookings.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|webp|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
